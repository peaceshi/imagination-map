{
  "name": "imagination-map",
  "version": "0.1.0",
  "description": "With a little imagination, the code would be better.",
  "scripts": {
    "prestart.old": "workbox generateSW workbox-config-dev.js",
    "start": "snowpack dev",
    "build": "snowpack build",
    "postbuild": "workbox generateSW workbox-config.js",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "check:format": "prettier --check ./",
    "format": "prettier --write ./",
    "eslint:lint": "eslint ./",
    "eslint:fix": "eslint ./ --fix",
    "stylelint:lint": "stylelint ./",
    "stylelint:fix": "stylelint ./ --fix"
  },
  "repository": "git+https://github.com/peaceshi/imagination-map.git",
  "author": "peaceshi <peaceshi@outlook.com> (https://github.com/peaceshi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peaceshi/imagination-map/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/*.{css,less,scss}": [
      "stylelint --config .stylelintrc.json --fix"
    ],
    "src/*.{css,scss,less,json,html,md,markdown}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@deck.gl/core": "^8.4.13",
    "@deck.gl/geo-layers": "^8.4.13",
    "@deck.gl/layers": "^8.4.13",
    "@deck.gl/mesh-layers": "^8.4.13",
    "@deck.gl/react": "^8.4.13",
    "@loaders.gl/core": "^2.3.13",
    "@loaders.gl/images": "^2.3.13",
    "@luma.gl/shadertools": "^8.4.4",
    "@math.gl/core": "^3.4.2",
    "@nebula.gl/edit-modes": "^0.22.3",
    "@nebula.gl/layers": "^0.22.3",
    "ahooks": "^2.10.0",
    "echarts": "^5.0.2",
    "echarts-for-react": "^3.0.1",
    "immer": "^9.0.1",
    "lodash.clonedeep": "^4.5.0",
    "nebula.gl": "^0.22.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stook": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@danmarshall/deckgl-typings": "^4.7.2",
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-react-refresh": "^2.4.2",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^2.3.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^11.2.6",
    "@types/chai": "^4.2.16",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/mocha": "^8.2.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-map-gl": "^5.2.9",
    "@types/snowpack-env": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@web/test-runner": "^0.12.18",
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-plugin-unicorn": "^29.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.1.2",
    "snowpack": "3.0.13",
    "stook-devtools": "^1.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-high-performance-animation": "^1.5.2",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.2.3",
    "workbox-cli": "^6.1.2",
    "workbox-precaching": "^6.1.2"
  }
}
